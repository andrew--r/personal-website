{% macro bookmarkView(bookmark) %}
<article class="bookmark h-entry" id="{{ bookmark.fileSlug }}">
  <h2><a href="{{ bookmark.data.url }}" class="p-name u-bookmark-of">{{ bookmark.data.title }}</a></h2>
  <p class="text_muted">{{ bookmark.data.url | hostFromUrl }}</p>

  {% if bookmark.templateContent %}
    <div class="e-content">{{ bookmark.templateContent | safe }}</div>
  {% endif %}
</article>
{% endmacro %}
